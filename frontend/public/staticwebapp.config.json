{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"
        ]
    },
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}